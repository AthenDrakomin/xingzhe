rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    // 媒体文件
    match /media/{mediaId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}